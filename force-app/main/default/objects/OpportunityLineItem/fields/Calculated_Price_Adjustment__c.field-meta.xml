<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Calculated_Price_Adjustment__c</fullName>
    <formula>IF(
    ISPICKVAL(Access_Type__c, &quot;Periodic&quot;), 
    (Aggregated_Price_Adjustment__c / Period_In_Months__c) / 100,
    
    IF(
        ISPICKVAL(Access_Type__c, &quot;Monthly&quot;), 
        (Aggregated_Price_Adjustment__c / 24) / 100,
        
        (Aggregated_Price_Adjustment__c / 1) / 100
    )
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Calculated Price Adjustment</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Percent</type>
</CustomField>
