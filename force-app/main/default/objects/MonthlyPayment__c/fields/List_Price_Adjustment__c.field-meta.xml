<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>List_Price_Adjustment__c</fullName>
    <formula>((FinalPrice__c / (
IF(
ISBLANK(OpportunityLineItem__c),
OrderItem__r.PricebookEntry.UnitPrice* Monthly_Units__c,
OpportunityLineItem__r.PricebookEntry.UnitPrice* Monthly_Units__c
)
)) - 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>List Price Adjustment %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
